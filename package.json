{
  "name": "@bmpickford/cem-plugin-docsify-gen",
  "version": "0.0.3",
  "description": "",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && tsc && cp -r src/templates/ ./dist",
    "start": "yarn build && nodemon dev.js --ext \"ts,js,json\"",
    "test": "uvu -r ts-node/register test",
    "test:watch": "watchlist src test -- yarn test",
    "docs:serve": "wds --root-dir=docs --node-resolve --watch",
    "test:jest": "jest"
  },
  "keywords": [
    "custom-elements",
    "custom-elements-json",
    "custom-elements-manifest",
    "customelements",
    "webcomponents",
    "customelementsjson",
    "customelementsmanifest"
  ],
  "author": "Benjamin Pickford",
  "license": "ISC",
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.1.14",
    "@types/jest": "^26.0.24",
    "@types/mustache": "^4.1.2",
    "@web/dev-server": "^0.1.18",
    "custom-elements-manifest": "^1.0.0",
    "globby": "^11.0.3",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "ts-node": "^10.1.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.2",
    "uvu": "^0.5.1",
    "watchlist": "^0.2.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bmpickford/cem-plugin-doc-gen.git"
  },
  "dependencies": {
    "jest": "^27.0.6",
    "mustache": "^4.2.0",
    "ts-jest": "^27.0.4"
  }
}
